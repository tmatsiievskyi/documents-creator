{
  "name": "documents-creator",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev --turbopack",
    "build": "next build",
    "start": "next start",
    "docker:dev:up": "docker compose -f docker/compose.dev.yaml up -d --build",
    "docker:dev:down": "docker compose -f docker/compose.dev.yaml down",
    "db:push": "drizzle-kit push --config=drizzle.config.ts",
    "db:generate": "drizzle-kit generate --config=drizzle.config.ts",
    "db:migrate": "drizzle-kit migrate --config=drizzle.config.ts",
    "db:drop": "drizzle-kit drop --config=drizzle.config.ts",
    "db:clear": "tsx ./src/db/clear.ts",
    "db:seed": "tsx ./src/db/seed.ts",
    "db:reset": "pnpm run db:clear && pnpm run db:migrate",
    "lint": "next lint",
    "lint:fix": "next lint --fix",
    "format": "prettier --write .",
    "format:check": "prettier --check .",
    "test": "jest --coverage --passWithNoTests",
    "test:watch": "jest --watch",
    "typecheck": "tsc --noEmit",
    "pre-commit": "pnpm run format && pnpm run lint:fix",
    "validate": "pnpm run format:check && pnpm run lint && pnpm run typecheck",
    "prepare": "husky"
  },
  "lint-staged": {
    "*.{js,jsx,ts,tsx}": [
      "prettier --write",
      "eslint --fix"
    ],
    "*.{json,css,md,yml,yaml}": [
      "prettier --write"
    ]
  },
